{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.cjs"
    }
  ],
  "builds": [
    {
      "src": "api/server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "outputDirectory": "dist",
  "installCommand": "npm install && cd api && npm install"
}